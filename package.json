{
  "name": "peoplepower",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@reduxjs/toolkit": "^1.2.1",
    "airtable": "^0.7.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "connected-react-router": "^6.6.1",
    "google-maps-react": "^2.0.2",
    "history": "^4.10.1",
    "moment": "^2.24.0",
    "qs": "^6.9.1",
    "react": "^16.10.1",
    "react-circular-progressbar": "^2.0.3",
    "react-dom": "^16.10.1",
    "react-modal": "^3.11.2",
    "react-moment": "^0.9.7",
    "react-number-format": "^4.4.1",
    "react-paypal-button-v2": "^2.4.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.1",
    "react-scripts": "3.1.2",
    "react-table-v6": "^6.8.6",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "semantic-ui-react": "^0.88.1",
    "victory": "^34.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "generate-schema": "generate-airtable-schema && pretty-quick",
    "deploy": "react-scripts build && mv ./build/index.html ./build/200.html &&surge ./build https://peoplepower.surge.sh"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "airtable-schema-generator": "^1.3.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "1.18.2",
    "pretty-quick": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ]
  },
  "airtable-schema-generator": {
    "output": "src/lib/airtable",
    "input": "src/lib/airtable",
    "defaultView": "Grid view",
    "mode": "auto-headless",
    "schemaMeta": {
      "Announcement": {
        "lookupFields": [
          "Project Group ID"
        ]
      },
      "Owner": {
        "lookupFields": [
          "Email",
          "Project Group ID"
        ]
      }
    }
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  }
}
